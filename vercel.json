{
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/main.jsx",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/server.js" },
      { "src": "/(.*)", "dest": "/src/index.html" }
    ]
  }
  